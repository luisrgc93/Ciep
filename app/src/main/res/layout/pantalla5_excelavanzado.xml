<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:weightSum="20"
    android:background="#ffffffff">


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="25" >



        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="30"
            android:weightSum="5" >
        </LinearLayout>


        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="2.2"
            android:weightSum="15" >
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="2.2"
                android:weightSum="15" >

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="6.2" >


                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="3" >
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1.5" >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"

                            android:layout_weight="2"
                            android:id="@+id/imagen1_pantalla5_excelavanzado"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="3" >
                    </LinearLayout>



                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="6.5" >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:text="Selecciona la opciÃ³n a la cual quieres accesar"
                        android:id="@+id/textView"
                        android:textStyle="bold"
                        android:textSize="17dp"
                        android:weightSum="1"
                        android:gravity="center"
                        android:textColor="#ff000000"
                        android:textIsSelectable="true"
                        android:layout_gravity="center_vertical" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="2.5"
                    android:weightSum="5">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/funciones_pantalla5_excelavanzado"
                        android:onClick="onClick">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"


                            android:id="@+id/imagen2_pantalla5_excelavanzado"
                            android:layout_gravity="left"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/referencias_pantalla5_excelavanzado"
                        android:onClick="onClick">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"

                            android:layout_weight="1"
                            android:id="@+id/imagen3_pantalla5_excelavanzado"
                            android:layout_gravity="left"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/filtrar_pantalla5_excelavanzado"
                        android:onClick="onClick">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"

                            android:layout_weight="1"
                            android:id="@+id/imagen4_pantalla5_excelavanzado"
                            android:layout_gravity="left"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/graficos_pantalla5_excelavanzado"
                        android:onClick="onClick">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"

                            android:layout_weight="1"
                            android:id="@+id/imagen5_pantalla5_excelavanzado"
                            android:layout_gravity="left"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/actividad_pantalla5_excelavanzado"
                        android:onClick="onClick">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"

                            android:layout_weight="1"
                            android:id="@+id/imagen6_pantalla5_excelavanzado"
                            android:layout_gravity="left"
                            />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="13.5">


                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"

                    android:id="@+id/anterior_pantalla5_excelavanzado"
                    android:onClick="onClick" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="30"
            android:weightSum="5" >
        </LinearLayout>

    </LinearLayout>
</LinearLayout>